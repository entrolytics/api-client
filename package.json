{
  "name": "@entrolytics/api-client",
  "version": "2.0.0",
  "description": "TypeScript API client for Entrolytics - First-party growth analytics for the edge",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^25.0.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "entrolytics",
    "analytics",
    "api",
    "client",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/entrolytics/api-client.git",
    "directory": "packages/api"
  },
  "license": "MIT"
}
